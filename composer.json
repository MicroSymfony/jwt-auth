{
    "name": "microsymfony/jwt-auth",
    "description": "A bundle for symfony designated to ease inter-service authentication using Json Web Tokens",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrius Petrauskis",
            "email": "andrius@petrauskis.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MicroSymfony\\JWT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroSymfony\\JWT\\Test\\": "tests/"
        }
    },
    "require": {
        "symfony/http-kernel": "^4.0",
        "lcobucci/jwt": "^3.2",
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
